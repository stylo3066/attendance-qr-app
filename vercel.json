{
  "version": 2,
  "builds": [
    {
      "src": "vercel-proxy/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "vercel-proxy/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "vercel-proxy/server.js"
    }
  ],
  "env": {
    "HMAC_SECRET": "dev_secret",
    "NODE_ENV": "production"
  }
}